    <param name="task_type" value="rope_table_opposite"/>

    <param name="rope_com_x" value="0.5"/>
    <param name="rope_com_y" value="0.2"/>
    <param name="rope_com_z" value="0.78"/>

    <param name="rope_num_links" value="29"/>
    <param name="rope_radius" value="0.005"/>
    <param name="rope_segment_length" value="0.02"/>
    
    <param name="constraint_translational_dir_deformability" value="5000.0"/>
